  313 symbols: flags "xkb_symbols" '{' symbolssect '}' ';'
  314        | flags "xkb_symbols" STR '{' symbolssect '}' ';'

  315 group: GROUPNUM
  316      | NUM
  317      | HEX

  318 key_list: key_list ',' KEYCODE
  319         | key_list ',' symbolname
  320         | KEYCODE
  321         | symbolname

  322 symbolinclude: '{' symbolssect '}'
  323              | symbolinclude '{' symbolssect '}'

  324 symbolssect: /* empty */
  325            | symbolssect vmods_def
  326            | symbolssect "name" '[' group ']' '=' STR ';'

  327 @45: /* empty */

  328 symbolssect: symbolssect "key" KEYCODE @45 '{' keydescs '}' ';'

