  276 @28: /* empty */

  277 action: "switchscreen" @28 '(' switchscrnparams ')'

  278 @29: /* empty */

  279 action: "consscroll" @29 '(' consscrollparams ')'

  280 @30: /* empty */

  281 action: "isolock" @30 '(' isolockparams ')'
  282       | "private" '(' privateparams ')'
  283       | "noaction" '(' ')'
  284       | error ')'

