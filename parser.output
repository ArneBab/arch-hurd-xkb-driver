  267 @24: /* empty */

  268 action: "lockptrbtn" @24 '(' ptrbtnparams ')'

  269 @25: /* empty */

  270 action: "setptrdflt" @25 '(' ptrdfltparams ')'

  271 @26: /* empty */

  272 action: "setcontrols" @26 '(' ctrlparams ')'

  273 @27: /* empty */

  274 action: "lockcontrols" @27 '(' ctrlparams ')'
