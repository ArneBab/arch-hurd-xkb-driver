Grammar

    0 $accept: xkbkeymap $end

    1 xkbkeymap: "xkb_keymap" '{' keymap '}' ';'
    2          | "xkb_keymap" STR '{' keymap '}' ';'

    3 @1: /* empty */

    4 xkbkeymap: '{' keymap '}' @1 ';'

    5 keymap: /* empty */
    6       | keymap types
    7       | keymap keycodes
    8       | keymap compat
    9       | keymap symbols
   10       | keymap geometry

   11 include: "include"
   12        | "augment"
   13        | "replace"
   14        | "override"

   15 flags: /* empty */
   16      | flags FLAGS

   17 keycodes: flags "xkb_keycodes" '{' keycodesect '}' ';'
   18         | flags "xkb_keycodes" STR '{' keycodesect '}' ';'

   19 keycodesinclude: '{' keycodesect '}'
   20                | keycodesinclude '{' keycodesect '}'

   21 keycodesect: /* empty */
   22            | "minimum" '=' NUM ';' keycodesect
   23            | "maximum" '=' NUM ';' keycodesect

   24 @2: /* empty */

   25 keycodesect: KEYCODE '=' NUM ';' @2 keycodesect

   26 @3: /* empty */

   27 keycodesect: "replace" KEYCODE '=' NUM ';' @3 keycodesect
   28            | "indicator" NUM '=' STR ';' keycodesect
   29            | "virtual" "indicator" NUM '=' STR ';' keycodesect

   30 @4: /* empty */

   31 keycodesect: "alias" KEYCODE '=' KEYCODE ';' @4 keycodesect

   32 @5: /* empty */

   33 keycodesect: include STR @5 keycodesinclude keycodesect

   34 types: flags "xkb_types" '{' typessect '}' ';'
   35      | flags "xkb_types" STR '{' typessect '}' ';'

   36 vmodslist: IDENTIFIER
   37          | vmodslist ',' IDENTIFIER

   38 vmods_def: "virtualmods" vmodslist ';'

   39 vmod: IDENTIFIER

   40 rmod: "shift"
   41     | "lock"
   42     | "control"
   43     | "mod1"
   44     | "mod2"
   45     | "mod3"
   46     | "mod4"
   47     | "mod5"

   48 mods: mods '+' rmod
   49     | mods '+' vmod

   50 @6: /* empty */

   51 mods: @6 rmod

   52 @7: /* empty */

   53 mods: @7 vmod
   54     | "all"
   55     | "none"

   56 level: LEVEL
   57      | "any"
   58      | NUM

   59 type: /* empty */
   60     | type "mods" '=' mods ';'
   61     | type "map" '[' mods ']' '=' level ';'
   62     | type "level_name" '[' level ']' '=' STR ';'
   63     | type "preserve" '[' mods ']' '=' mods ';'

   64 typesinclude: '{' typessect '}'
   65             | typesinclude '{' typessect '}'

   66 typessect: /* empty */
   67          | typessect vmods_def

   68 @8: /* empty */

   69 typessect: typessect "type" STR @8 '{' type '}' ';'

   70 @9: /* empty */

   71 typessect: typessect include STR @9 typesinclude

   72 compat: flags "xkb_compatibility" '{' compatsect '}' ';'
   73       | flags "xkb_compatibility" STR '{' compatsect '}' ';'

   74 symbol: IDENTIFIER
   75       | "any"
   76       | error

   77 whichstate: "whichstate"
   78           | "none"
   79           | "any"

   80 groups: groups '+' group
   81       | groups '-' group
   82       | group
   83       | "all"

   84 indicators: /* empty */
   85           | indicators indicator

   86 indicator: "mods" '=' mods ';'
   87          | "groups" '=' groups ';'
   88          | "controls" '=' ctrls ';'
   89          | "whichmodstate" '=' whichstate ';'
   90          | "whichgroupstate" '=' whichstate ';'
   91          | allowexplicit ';'
   92          | driveskbd ';'
   93          | "index" '=' NUM ';'

   94 allowexplicit: '~' "allowexplicit"
   95              | '!' "allowexplicit"
   96              | "allowexplicit"
   97              | "allowexplicit" '=' BOOLEAN

   98 driveskbd: '~' "driveskbd"
   99          | '!' "driveskbd"
  100          | "driveskbd"
  101          | "driveskbd" '=' BOOLEAN

  102 interprets: /* empty */
  103           | interprets interpret

  104 interpret: "usemodmap" '=' level ';'
  105          | repeat ';'
  106          | locking ';'
  107          | "virtualmod" '=' vmod ';'
  108          | "action" '=' action ';'

  109 compatinclude: '{' compatsect '}'
  110              | compatinclude '{' compatsect '}'

  111 compatsect: /* empty */
  112           | compatsect vmods_def

  113 @10: /* empty */

  114 compatsect: compatsect "interpret" '.' @10 interpret

  115 @11: /* empty */

  116 compatsect: compatsect "interpret" symbol @11 '{' interprets '}' ';'

  117 @12: /* empty */

  118 compatsect: compatsect "interpret" symbol '+' rmod @12 '{' interprets '}' ';'

  119 @13: /* empty */

  120 compatsect: compatsect "interpret" symbol '+' "any" @13 '{' interprets '}' ';'

  121 @14: /* empty */

  122 compatsect: compatsect "interpret" symbol '+' INTERPMATCH '(' mods ')' @14 '{' interprets '}' ';'
  123           | compatsect "group" NUM '=' mods ';'
  124           | compatsect "indicator" STR '{' indicators '}' ';'

  125 @15: /* empty */

  126 compatsect: compatsect include STR @15 compatinclude
  127           | compatsect actiondef
  128           | compatsect "indicator" '.' indicator

  129 clearlocks: '~' "clearlocks"
  130           | '!' "clearlocks"
  131           | "clearlocks"
  132           | "clearlocks" '=' BOOLEAN

  133 latchtolock: '~' "latchtolock"
  134            | '!' "latchtolock"
  135            | "latchtolock"
  136            | "latchtolock" '=' BOOLEAN

  137 usemodmap: '~' "usemodmap"
  138          | '!' "usemodmap"
  139          | "usemodmap"
  140          | "usemodmap" '=' BOOLEAN

  141 locking: '~' "locking"
  142        | '!' "locking"
  143        | "locking"
  144        | "locking" '=' BOOLEAN

  145 repeat: '~' "repeat"
  146       | '!' "repeat"
  147       | "repeat"
  148       | "repeat" '=' BOOLEAN

  149 groupswrap: '~' "groupswrap"
  150           | '!' "groupswrap"
  151           | "groupswrap"
  152           | "groupswrap" '=' BOOLEAN

  153 groupsclamp: '~' "groupsclamp"
  154            | '!' "groupsclamp"
  155            | "groupsclamp"
  156            | "groupsclamp" '=' BOOLEAN

  157 noaccel: '~' "accel"
  158        | '!' "accel"
  159        | "accel"
  160        | "accel" '=' BOOLEAN

  161 sameserver: '~' "sameserver"
  162           | '!' "sameserver"
  163           | "sameserver"
  164           | "sameserver" '=' BOOLEAN

  165 setmodsparams: /* empty */
  166              | setmodsparam
  167              | setmodsparams ',' setmodsparam

  168 setmodsparam: "mods" '=' mods
  169             | "mods" '=' "usemodmap"
  170             | clearlocks
  171             | usemodmap
  172             | latchtolock

  173 setgroupparams: /* empty */
  174               | setgroupparam
  175               | setgroupparams ',' setgroupparam

  176 setgroupparam: "group" '=' NUM
  177              | "group" '=' '+' NUM
  178              | "group" '=' '-' NUM
  179              | clearlocks
  180              | latchtolock

  181 moveptrparams: /* empty */
  182              | moveptrparam
  183              | moveptrparams ',' moveptrparam

  184 moveptrparam: IDENTIFIER '=' NUM
  185             | IDENTIFIER '=' '+' NUM
  186             | IDENTIFIER '=' '-' NUM
  187             | noaccel

  188 button: NUM
  189       | BUTTONNUM
  190       | "default"

  191 affectbtnlock: "lock"
  192              | "unlock"
  193              | "both"
  194              | "neither"

  195 ptrbtnparams: /* empty */
  196             | ptrbtnparam
  197             | ptrbtnparams ',' ptrbtnparam

  198 ptrbtnparam: "button" '=' button
  199            | "count" '=' NUM
  200            | "affect" '=' affectbtnlock

  201 affectbtns: "defaultbutton"
  202           | "button"
  203           | "all"

  204 ptrdfltparams: /* empty */
  205              | ptrdfltparam
  206              | ptrdfltparams ',' ptrdfltparam

  207 ptrdfltparam: "button" '=' button
  208             | "button" '=' '+' button
  209             | "button" '=' '-' button
  210             | "affect" '=' affectbtns

  211 ctrls: ctrls '+' CONTROLFLAG
  212      | CONTROLFLAG
  213      | "overlay"

  214 ctrlflags: ctrls
  215          | "all"
  216          | "none"

  217 ctrlparams: "controls" '=' ctrlflags

  218 isoaffect: "mods"
  219          | "groups"
  220          | "controls"
  221          | "pointers"
  222          | "all"
  223          | "none"

  224 isolockparams: /* empty */
  225              | isolockparam
  226              | isolockparams ',' isolockparam

  227 isolockparam: "mods" '=' mods
  228             | "mods" '=' "usemodmap"
  229             | "group" '=' group
  230             | "controls" '=' ctrlflags
  231             | "affect" '=' isoaffect

  232 switchscrnparams: switchscrnparam
  233                 | switchscrnparams ',' switchscrnparam

  234 switchscrnparam: "screen" '=' NUM
  235                | "screen" '+' '=' NUM
  236                | "screen" '-' '=' NUM
  237                | sameserver

  238 consscrollparams: consscrollparam
  239                 | consscrollparams ',' consscrollparam

  240 consscrollparam: "screen" '+' '=' "float"
  241                | "screen" '-' '=' "float"
  242                | "line" '=' NUM
  243                | "line" '+' '=' NUM
  244                | "line" '-' '=' NUM
  245                | "percent" '=' NUM

  246 privateparams: /* empty */
  247              | privateparam
  248              | privateparams ',' privateparam

  249 privateparam: "type" '=' HEX
  250             | "data" '=' STR

  251 @16: /* empty */

  252 action: "setmods" @16 '(' setmodsparams ')'

  253 @17: /* empty */

  254 action: "latchmods" @17 '(' setmodsparams ')'

  255 @18: /* empty */

  256 action: "lockmods" @18 '(' setmodsparams ')'

  257 @19: /* empty */

  258 action: "setgroup" @19 '(' setgroupparams ')'

  259 @20: /* empty */

  260 action: "latchgroup" @20 '(' setgroupparams ')'

  261 @21: /* empty */

  262 action: "lockgroup" @21 '(' setgroupparams ')'

  263 @22: /* empty */

  264 action: "moveptr" @22 '(' moveptrparams ')'

